              ],
              "click": null,
              "icon": "RiCodeSSlashFill",
              "values": {
                "comment": "",
                "alias": "read batches",
                "debug": true,
                "propagate": true,
                "notify_warnings": true,
                "code": "tot = data['COUNT(*)']\nbatch_size = 3\nn_batches = tot/batch_size\nn_batches = n_batches if tot%batch_size==0 else int(n_batches+1)\nfor i in range(n_batches):\n  offset = i*batch_size\n  yield f'SELECT * FROM mytable LIMIT {batch_size} OFFSET {offset}'\n\n"
              }
            },
